        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "vitest run",
        "prepare": "npm run build",
        "prepublishOnly": "npm test",
        "publish": "npm run build && npm publish",
        "typecheck": "vue-tsc --noEmit",
        "typecheck:watch": "vue-tsc --noEmit --watch"
    },
    "dependencies": {
        "@codemirror/lang-javascript": "^6.2.2",
